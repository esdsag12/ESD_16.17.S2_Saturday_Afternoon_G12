<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue"
    android:orientation="vertical"
    android:transitionName="lnPract"
    tools:context="com.app.esd.esd.Activity.PracticeActivity">

    <ImageView
        android:id="@+id/imgv_pract"
        android:layout_width="100dp"
        android:layout_gravity="center_horizontal"
        android:layout_height="100dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="16dp"
        android:fitsSystemWindows="true"
        android:src="@drawable/desk"
        android:transitionName="icPract" />

    <android.support.v4.view.ViewPager
        android:theme="@style/PractViewPager"
        android:id="@+id/viewpager_pract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:layout_marginTop="32dp" />
</LinearLayout>
